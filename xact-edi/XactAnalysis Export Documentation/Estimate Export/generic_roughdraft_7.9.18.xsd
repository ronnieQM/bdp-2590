<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://xactware.com/generic_roughdraft.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified" id="GENERIC_ROUGHDRAFT" xmlns:xact="http://xactware.com/generic_roughdraft.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="GENERIC_ROUGHDRAFT" >
		<xs:annotation>
			<xs:documentation>Generic Roughdraft XML format for Xactimate.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="COVERSHEET"   minOccurs="0" maxOccurs="1"  >
					<xs:annotation>
						<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDGenCoversheetDoc)</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="CONTACTS"   minOccurs="0" maxOccurs="1"  >
								<xs:annotation>
									<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDGenContactsDoc)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="CONTACT"   minOccurs="0" maxOccurs="unbounded"  >
											<xs:annotation>
												<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDGenContactDoc)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:all>
													<xs:element name="ADDRESSES"   minOccurs="0" maxOccurs="1"  >
														<xs:annotation>
															<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDAddressesDoc)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="ADDRESS"   minOccurs="0" maxOccurs="unbounded"  >
																	<xs:annotation>
																		<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDAddressDoc)</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:attribute name="zip" use="optional" >
																			<xs:annotation>
																				<xs:documentation>Zip (postal) code part of the address.(Example: 84097-1111)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:minLength value="0" />
																					<xs:maxLength value="10" />
																					<xs:pattern value="[^a-z]*" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="type" use="optional" >
																			<xs:annotation>
																				<xs:documentation>The type of address (Example: Property, Billing, etc.)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:minLength value="0" />
																					<xs:maxLength value="12" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="street" use="optional" >
																			<xs:annotation>
																				<xs:documentation>Street part of the address</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:minLength value="0" />
																					<xs:maxLength value="60" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="city" use="optional" >
																			<xs:annotation>
																				<xs:documentation>City part of the address</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:minLength value="0" />
																					<xs:maxLength value="30" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="state" use="optional" >
																			<xs:annotation>
																				<xs:documentation>State part of the address. (Example: UT)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:minLength value="0" />
																					<xs:maxLength value="2" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="primary" type="xact:xbool" use="optional" >
																			<xs:annotation>
																				<xs:documentation>Is this address the primary address?</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="PHONES"   minOccurs="0" maxOccurs="1"  >
														<xs:annotation>
															<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDPhonesDoc)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="PHONE"   minOccurs="0" maxOccurs="unbounded"  >
																	<xs:annotation>
																		<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDPhoneDoc)</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:attribute name="type" use="optional" >
																			<xs:annotation>
																				<xs:documentation>The type of phone number.(Example: Home, Cell, etc.)</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:minLength value="0" />
																					<xs:maxLength value="12" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="phone" use="optional" >
																			<xs:annotation>
																				<xs:documentation>The phone number.  It contains dashes between the numbers NNN-NNN-NNNN or NNN-NNNN.</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:minLength value="0" />
																					<xs:maxLength value="20" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="ext" use="optional" >
																			<xs:annotation>
																				<xs:documentation>The extension part of the phone number</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:minLength value="0" />
																					<xs:maxLength value="20" />
																					<xs:pattern value="[0-9]*" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="primary" type="xact:xbool" use="optional" >
																			<xs:annotation>
																				<xs:documentation>Is this phone the primary phone number?</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:all>
												<xs:attribute name="type" use="optional" >
													<xs:annotation>
														<xs:documentation>Type of contact. (Example: Claim Rep, Estimator, etc.)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="50" />
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="name" use="optional" >
													<xs:annotation>
														<xs:documentation>Contact name</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="160" />
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="title" use="optional" >
													<xs:annotation>
														<xs:documentation>Contact title (position)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="30" />
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="company" use="optional" >
													<xs:annotation>
														<xs:documentation>Name of company this contact is associated with</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="160" />
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="DATES"   minOccurs="0" maxOccurs="1"  >
								<xs:annotation>
									<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDGenDatesDoc)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="loss" type="xact:XWDateTime" use="optional" >
										<xs:annotation>
											<xs:documentation>Date and time of Loss</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="inspected" type="xact:XWDateTime" use="optional" >
										<xs:annotation>
											<xs:documentation>Date and time Inspected</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="completed" type="xact:XWDateTime" use="optional" >
										<xs:annotation>
											<xs:documentation>Date and time Completed</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="received" type="xact:XWDateTime" use="optional" >
										<xs:annotation>
											<xs:documentation>Date and time Received</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="entered" type="xact:XWDateTime" use="optional" >
										<xs:annotation>
											<xs:documentation>Date and time Entered</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="contacted" type="xact:XWDateTime" use="optional" >
										<xs:annotation>
											<xs:documentation>Date and time Contacted</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="ESTIMATE_INFO"   minOccurs="0" maxOccurs="1"  >
								<xs:annotation>
									<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDGenEstimateInfoDoc)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:all>
										<xs:element name="EST_CHANGES"   minOccurs="0" maxOccurs="1"  >
											<xs:annotation>
												<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDGenEstChangesDoc)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="EST_CHANGE"   minOccurs="0" maxOccurs="unbounded"  >
														<xs:annotation>
															<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDGenEstChangeDoc)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="reason" type="xs:string" use="optional" >
																<xs:annotation>
																	<xs:documentation>The reason for the change</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="onsite" type="xact:xbool" use="optional" >
																<xs:annotation>
																	<xs:documentation>Was this onsite change?</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:all>
									<xs:attribute name="estimateType" type="xs:string" use="optional" >
										<xs:annotation>
											<xs:documentation>The type of estimate.  (Example: Mixed, Structural, Contents)</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="deprMat" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Whether the Depreciate Materials setting is turned on in the estimate</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="deprNonMat" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Whether the Depreciate Non-Materials setting is turned on in the estimate</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="deprRemoval" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Whether the Depreciate Removal setting is turned on in the estimate</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="deprOandP" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Whether the Depreciate O and P setting is turned on in the estimate</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="deprTaxes" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Whether the Depreciate Taxes setting is turned on in the estimate</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="inspNotPerformed" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Whether an inspection was NOT performed</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="roofDamage" type="xact:CovLossInfoDoc.RoofReplacementType" use="optional" >
										<xs:annotation>
											<xs:documentation>One of: "0" for None, "1" for PartialReplacement, "2" for TotalReplacement</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="scopeType" use="optional" >
										<xs:annotation>
											<xs:documentation>How the loss was scoped - whether it be drone, onsite, etc.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="35" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="estimateCount" type="xs:integer" use="optional" >
										<xs:annotation>
											<xs:documentation>The estimate interations from XactAnalysis</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="onsite" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Was this an onsite inspection?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="printOnsite" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Was the estimate printed onsite?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="onsitePayment" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Was there an onsite payment?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="estimatedOnsite" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Was estimated onsite?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="isTotalLoss" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Was this a total loss?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="backInProgress" type="xact:RDGenEstimateInfoDoc.BackInProgressEnum" use="optional" >
										<xs:annotation>
											<xs:documentation>One of: "0" for NotApplicable, "1" for Supplement, "2" for Correction</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="afo" use="optional" >
										<xs:annotation>
											<xs:documentation>Adjusting Field Office</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="3" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="xsImported" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Was XactScope imported into this estimate?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="recipientsXNAddress" type="xs:string" use="optional" >
										<xs:annotation>
											<xs:documentation>Recipient's Xactnet address</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="causeOfLoss" use="optional" >
										<xs:annotation>
											<xs:documentation>Cause of Loss</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="256" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="otherCause" use="optional" >
										<xs:annotation>
											<xs:documentation>Cause of Loss other field</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="256" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="rotationTrade" type="xs:string" use="optional" >
										<xs:annotation>
											<xs:documentation>Job Type</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="carrierId" type="xs:string" use="optional" >
										<xs:annotation>
											<xs:documentation>Code identifying the Profile</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="originalTransactionId" type="xs:string" use="optional" >
										<xs:annotation>
											<xs:documentation>Original Transaction ID (MFN) from XA</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="usedAerialSketch" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Was Aerial Sketch used?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="policyNumber" use="optional" >
										<xs:annotation>
											<xs:documentation>Policy Number</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="20" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="claimNumber" use="optional" >
										<xs:annotation>
											<xs:documentation>Claim Number</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="30" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="estimateName" use="optional" >
										<xs:annotation>
											<xs:documentation>Estimate Name.  This is the main identifier for the estimate.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="20" />
												<xs:pattern value="[^a-z\s]*" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="insuredName" use="optional" >
										<xs:annotation>
											<xs:documentation>Insured Name</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="160" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="priceList" use="optional" >
										<xs:annotation>
											<xs:documentation>The price list that is used for the estimate</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="20" />
												<xs:pattern value="[^a-z\s]*" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="laborEff" use="optional" >
										<xs:annotation>
											<xs:documentation>Labor Efficiency Text</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="100" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="typeOfLoss" use="optional" >
										<xs:annotation>
											<xs:documentation>Type of Loss</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="35" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="laborCostModel" type="xact:LCMChoice" use="optional" >
										<xs:annotation>
											<xs:documentation>LaborMinimums, BSC or None</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="waterLossCategory" type="xact:CovLossInfoDoc.WaterLossCategory" use="optional" >
										<xs:annotation>
											<xs:documentation>None, One, Two, Three or Unknown</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="waterLossClass" type="xact:CovLossInfoDoc.WaterLossClass" use="optional" >
										<xs:annotation>
											<xs:documentation>None, Class 1, Class 2, Class 3, Class 4, or Unknown</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="ADDRESSES"   minOccurs="0" maxOccurs="1"  >
								<xs:annotation>
									<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDAddressesDoc)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ADDRESS"   minOccurs="0" maxOccurs="unbounded"  >
											<xs:annotation>
												<xs:documentation>(typeOf:Xm8.Data.ProjDoc.RDAddressDoc)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="zip" use="optional" >
													<xs:annotation>
														<xs:documentation>Zip (postal) code part of the address.(Example: 84097-1111)</xs:documentation>
													</xs:annotation>
