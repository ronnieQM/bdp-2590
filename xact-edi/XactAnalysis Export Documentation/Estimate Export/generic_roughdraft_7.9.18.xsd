															</xs:attribute>
															<xs:attribute name="rcvTotal" type="xs:decimal" use="optional" >
																<xs:annotation>
																	<xs:documentation>Total RCV with tax and addons for this item activity. Only available if the item uses more than one activity</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="NOTE"  minOccurs="0" maxOccurs="1"  >
											<xs:annotation>
												<xs:documentation>Item note</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:normalizedString" >
													<xs:minLength value="0"/>
													<xs:maxLength value="10000"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:all>
									<xs:attribute name="actPrefix" use="optional" >
										<xs:annotation>
											<xs:documentation>Text representation of the activity that is prefixed on the item description in the printed report</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="50" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="isHomeowner" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Is this a homeowner supplied item?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="isCredit" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Is this a credit item?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="containsBSCFactoredIn" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Does this item use components that have BSCs set to "Factored In"? (Only applies when the labor efficiencies for the estimate are set to "Broken Out")</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="containsBSCDontApply" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Does this item use components that have BSCs set to "Do not Apply"?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="jobTaxType" use="optional" >
										<xs:annotation>
											<xs:documentation>The job tax type.  The possible unit cost indicator characters are:  *ENFDH.  * means the price has changed.  E means the item is tax exempt.  N means that overhead and profit will not be applied to the item.  F means the item contains one or more labor components with base service charges factored in.  D means the item contains one or more labor components with base service charges that do not apply.  H  means the item is a homeowner item.  In addition, if the sales taxes being used contain more than one tax job type, the applicable tax job type code will be appended to the end of the cost indicator text.  Country, State, First Job Type, First Job Type Code, Second Job Type, Second Job Type Code|USA, AR, Material Only, (MO), Replacement Svc Tax, (ST)|USA, IA, Non-repair, (NR), Repair, (RP)|USA, KS, Residential, ( ), Commercial Rpr/Rmdl, (CO)|Canada, MB, Struct &amp; Cont Repl, (SC), Cont Clean &amp; Mech, (CC)|USA, NJ, Repair-Maintenance, ( ), Capital Improvement, (CI)|USA, NY, Non-Capital Improv., ( ), Capital Improvement, (CI)|USA, TX, Residential, (RS), Carpet disp-Water, (CW)|USA, WI, Non-service, ( ), Service, (SE)|USA, WV, Non Capital Impr., ( ), Capital Improvement, (CI)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="40" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="laborTotal" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Total labor amount for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="laborBase" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Labor base wage amount for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="laborBurden" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Labor burden amount for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="laborMarkup" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Labor markup amount for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="laborHours" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Total labor hours for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="material" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Material amount for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="equipment" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Equipment amount for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="marketCond" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Market Conditions for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="acv" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>ACV for the item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="lineNum" use="optional" >
										<xs:annotation>
											<xs:documentation>Line item number.  (Examples: "1", "2", "3a", "3b", etc.)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="11" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="sublimitName" use="optional" >
										<xs:annotation>
											<xs:documentation>The name of the sublimit this item belongs to</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="100" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="tax" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Total tax for this item, including tax on addons</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="addons" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Total addons for this item, including addons on tax</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="deprTotal" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Total depreciation with tax and addons for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="acvTotal" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Total ACV with tax and addons for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="rcvTotal" type="xs:decimal" use="optional" >
										<xs:annotation>
											<xs:documentation>Total RCV with tax and addons for this item</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="isPartOfInitSettle" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Is this item part of the initial settlement?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="type" use="optional" >
										<xs:annotation>
											<xs:documentation>Type of item. Either "I" for item or "N" for note</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="1" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="cat" use="optional" >
										<xs:annotation>
											<xs:documentation>Category code for the item</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="3" />
												<xs:pattern value="[^a-z\s]*" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="sel" use="optional" >
										<xs:annotation>
											<xs:documentation>Selector code for the item</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="10" />
												<xs:pattern value="[^a-z]*" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="act" use="optional" >
										<xs:annotation>
											<xs:documentation>Activity code for the item</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="1" />
												<xs:pattern value="[^\s]*" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="desc" use="optional" >
										<xs:annotation>
											<xs:documentation>Description for the item</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="1024" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="descChanged" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Does the description in the estimate differ from the price list?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="calc" type="xs:string" use="optional" >
										<xs:annotation>
											<xs:documentation>Calculation for the item. The limit for the calculation in the line item ui is 256, but it can be much more when sketch builds the string, so this string is not given a maximum length.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="qty" use="optional" >
										<xs:annotation>
											<xs:documentation>Quantity of the item</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:decimal">
												<xs:fractionDigits value="2" />
												<xs:maxInclusive value="999999999.99" />
												<xs:minInclusive value="-999999999.99" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="unit" use="optional" >
										<xs:annotation>
											<xs:documentation>Units for the item</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="2" />
												<xs:pattern value="[^a-z\s]*" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="unitChanged" type="xact:xbool" use="optional" >
										<xs:annotation>
											<xs:documentation>Does the unit in the estimate differ from the unit in the price list?</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="remove" use="optional" >
										<xs:annotation>
											<xs:documentation>Cost for removal of the item, the "-" activity</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:decimal">
												<xs:fractionDigits value="2" />
												<xs:maxInclusive value="999999999.99" />
												<xs:minInclusive value="-999999999.99" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="replace" use="optional" >
										<xs:annotation>
											<xs:documentation>Unit price for the replace (all activities that are not " - ") portion of this item</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:decimal">
												<xs:fractionDigits value="2" />
												<xs:maxInclusive value="999999999.99" />
												<xs:minInclusive value="-999999999.99" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
