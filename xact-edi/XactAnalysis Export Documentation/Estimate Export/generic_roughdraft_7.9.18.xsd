							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:fractionDigits value="2" />
									<xs:maxInclusive value="999999999.99" />
									<xs:minInclusive value="0" />
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="sfWalls" use="optional" >
							<xs:annotation>
								<xs:documentation>Square footage of walls</xs:documentation>
