	</xs:complexType>
	<xs:complexType name="ADDRESSES">
		<xs:sequence>
			<xs:element name="ADDRESS" type="ADDRESS" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ADM">
		<xs:all>
			<xs:element name="COVERAGE_LOSS" type="COVERAGE_LOSS" minOccurs="0"/>
		</xs:all>
		<xs:attribute name="agentCode" type="str8" use="optional"/>
		<xs:attribute name="agentName" type="str85" use="optional"/>
		<xs:attribute name="branchNumber" type="nullableInteger" use="optional"/>
		<xs:attribute name="clientNumber" type="nullableInteger" use="optional"/>
		<xs:attribute name="dateEntered" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="dateProjCompleted" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="dateJobCompleted" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="dateContacted" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="dateInspected" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="dateOfLoss" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="dateReceived" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="fileNumber" type="str36" use="optional"/>
		<xs:attribute name="onsite" type="bool" use="optional" default = "0"/>
		<xs:attribute name="estimatedOnsite" type="bool" use="optional" default = "0"/>
		<xs:attribute name="printOnsite" type="bool" use="optional" default = "0"/>
		<xs:attribute name="onsitePayment" type="bool" use="optional" default = "0"/>
		<xs:attribute name="numRmsAffected" type="nullableInteger" use="optional"/>
		<xs:attribute name="denial" type="denialType" use="optional"/>
		<xs:attribute name="denialExplanation" type="str4000" use="optional"/>
		<xs:attribute name="dateProjectedComplete" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="dateProjectedStart" type="dateOrDateTime" use="optional"/>
