<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="XACTDOC" type="XACTDOC"/>
	<xs:complexType name="ACTIVITIES">
		<xs:annotation>
			<xs:documentation>This element and its children are included if the "Add Activity Diary Notes" option is selected as an export option</xs:documentation>
			<xs:documentation>CSR: Make sure the major version of the export is 2003 or above, if the major version is left blank or is 2002 or below then older depreciated tags are included in the XML and they aren't part of this schema</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ACTIVITY" type="ACTIVITY" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ACTIVITY">
		<xs:sequence>
			<xs:element name="EXPENSES" type="EXPENSES" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="process" type="str100" use="optional"/>
		<xs:attribute name="activity" type="str20" use="optional"/>
		<xs:attribute name="start" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="finish" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="updated" type="dateOrDateTime" use="optional"/>
		<xs:attribute name="desc" type="str100" use="optional"/>
		<xs:attribute name="longDesc" type="str8000" use="optional"/>
		<xs:attribute name="remind" type="bool" use="optional"/>
		<xs:attribute name="todo" type="bool" use="optional"/>
	</xs:complexType>
	<xs:complexType name="ADDRESS">
		<xs:attribute name="city" type="str60" use="optional"/>
		<xs:attribute name="country" type="country" use="optional"/>
        <xs:attribute name="county" type="str60" use="optional"/>
		<xs:attribute name="lat" type="nullableDecimal" use="optional"/>
		<xs:attribute name="lon" type="nullableDecimal" use="optional"/>
		<xs:attribute name="postal" type="str10" use="optional"/>
        <xs:attribute name="primary" type="bool" use="optional"/>
		<xs:attribute name="state" type="str60" use="optional"/>
		<xs:attribute name="street" type="str60" use="optional"/>
		<xs:attribute name="street2" type="str60" use="optional"/>
		<xs:attribute name="street3" type="str60" use="optional"/>
