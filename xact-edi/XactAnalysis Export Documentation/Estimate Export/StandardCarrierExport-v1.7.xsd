		<xs:attribute name="goesBy" type="str20" use="optional"/>
		<xs:attribute name="position" type="str25" use="optional"/>
		<xs:attribute name="mortgageHolder" type="str80" use="optional">
			<xs:annotation>
				<xs:documentation>Only included for the "Client" type if not blank</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="mortgageLoanNum" type="str15" use="optional">
			<xs:annotation>
				<xs:documentation>Only included for the "Client" type if not blank </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="birthDate" type="dateOrNull" use="optional"/>
		<xs:attribute name="reference" type="str60" use="optional"/>
		<xs:attribute name="language" type="str20" use="optional"/>
		<xs:attribute name="title" type="str60" use="optional"/>
		<xs:attribute name="claimUnit" type="str5" use="optional">
			<xs:annotation>
				<xs:documentation>Only included for the "Estimator" or "ClaimRep" type if not blank </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CONTACTMETHODS">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="EMAIL" type="EMAIL" minOccurs="0" maxOccurs="1"/>
			<xs:element name="PHONE" type="PHONE" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CONTACTS">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:choice>
				<xs:element name="CONTACT" type="CONTACT"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CONTROL_POINT">
		<xs:all>
			<xs:element name="NOTES" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Notes associated with the control point. Not all control points contain notes. One note per control point.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:attribute name="reservation" type="bool" use="optional">
			<xs:annotation>
				<xs:documentation>If the control point is the reviewed control point, this attribute indicates whether the record has been reviewed with reservations/exceptions.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="stamp" type="nullableDateOrDateTime" use="optional">
			<xs:annotation>
				<xs:documentation>The date and time of the control point.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="type" type="controlPointType" use="optional"/>
		<xs:attribute name="user" type="str55" use="optional"/>
		<xs:attribute name="userNumber" type="str25" use="optional"/>
		<xs:attribute name="userId" type="str20" use="optional"/>
		<xs:attribute name="flags" type="nullableInteger" use="optional"/>
		<xs:attribute name="amount" type="nullableDecimal" use="optional"/>
		<xs:attribute name="userLastName" type="str40" use="optional"/>
		<xs:attribute name="userFirstName" type="str40" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CONTROL_POINTS">
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="CANCELLATION_COMMENTS" type="xs:string">
					<xs:annotation>
						<xs:documentation>Added if the "Add Control Points" Option is on</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CONTROL_POINT" type="CONTROL_POINT">
					<xs:annotation>
						<xs:documentation>Added if the "Add Control Points" Option is on or XN Standard Data is included</xs:documentation>
						<xs:documentation>Added if XN Standard Data is included  for carrier or sender desk adjuster assigned checkpoints</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="grossEstimate" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation>Added if the "Add Control Points" Option is on</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="bill" type="nullableInteger" use="optional">
			<xs:annotation>
				<xs:documentation>Added if the "Add Control Points" Option is on</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="referral" type="bool" use="optional">
			<xs:annotation>
				<xs:documentation>Added if the "Add Control Points" Option is on</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="testAssignment" type="bool" use="optional" default="0">
			<xs:annotation>
				<xs:documentation>Included in export regardless of whether the "Add Control Points" Option is on</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
