			<xs:annotation>
				<xs:documentation>UK only</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="gt500PersonDays" type="nullableInteger" use="optional">
			<xs:annotation>
				<xs:documentation>UK only</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ADMIN_INFO">
		<xs:attribute name="branch" type="str20" use="optional"/>
		<xs:attribute name="accessName" type="str60" use="optional"/>
		<xs:attribute name="accessPhone" type="phoneNumber" use="optional"/>
		<xs:attribute name="accessPhoneExt" type="phoneExt20" use="optional"/>
	</xs:complexType>
	<xs:complexType name="ATT_FILE">
		<xs:attribute name="fileName" type="str200" use="required"/>
		<xs:attribute name="fileDesc" type="str2048" use="required"/>
        <xs:attribute name="fileType" type="str200" use="optional"/>
		<xs:attribute name="fileDate" type="nullableDateOrDateTime" use="optional"/>
		<xs:attribute name="approvalStatus" type="attachmentApprovalType" use="optional">
			<xs:annotation>
				<xs:documentation>Included as part of the Add approved status option (a sub-option of the Add List Of Exported Files option)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ATTACHMENTS">
		<xs:annotation>
			<xs:documentation>Included if the Add List of Exported Files option (in XML) is selected</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ATT_FILE" type="ATT_FILE" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CLAIM_INFO">
		<xs:all>
			<xs:element name="ADMIN_INFO" type="ADMIN_INFO" minOccurs="0"/>
			<xs:element name="ACTIVITIES" type="ACTIVITIES" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="COL">
		<xs:attribute name="desc" type="str256" use="optional"/>
		<xs:attribute name="otherCause" type="str256" use="optional"/>
	</xs:complexType>
	<xs:complexType name="COMPANY">
		<xs:attribute name="name" type="str40" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CONTACT">
		<xs:all>
			<xs:element name="COMPANY" type="COMPANY" minOccurs="0"/>
			<xs:element name="ADDRESSES" type="ADDRESSES" minOccurs="0"/>
			<xs:element name="CONTACTMETHODS" type="CONTACTMETHODS" minOccurs="0"/>
		</xs:all>
